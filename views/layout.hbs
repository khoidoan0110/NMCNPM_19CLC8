<!-- body -->

<body class="main-layout">
    <!-- loader  -->

    <div class="wrapper">
        <div id="content">
            <!-- header -->
            <header>
                <!-- header inner -->
                <div class="header">

                    <div class="container-fluid">

                        <div class="row">
                            <div class="col-lg-3 logo_section">
                                <div class="full">
                                    <div class="center-desk">
                                        <div class="logo">
                                            <a href="/"><img src="/images/logo.jpg" alt="#"></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-9">
                                <div class="right_header_info">
                                    <div class="form-group has-search" style="display: inline-block;float:left;">
                                        <span class="fa fa-search form-control-feedback"></span>
                                        <form action="/product/search" method="GET">
                                            <input type="search" class="form-control" placeholder="Search"
                                                name="search" />
                                        </form>
                                    </div>
                                    <ul>
                                        <li>
                                            <div class="dropdown" style="display: inline;">
                                                <button class="btn btn-secondary" type="button" id="dropdownMenu2"
                                                    data-toggle="dropdown" style="display: inline; margin-right: 15px">
                                                    Account
                                                </button>
                                                {{#if user}}
                                                <div class="dropdown-menu" aria-labelledby="dropdownMenu2">
                                                    <a class="dropdown-item" href="/user/information">User
                                                        information</a>
                                                    <a class="dropdown-item" href="/user/order">Order</a>
                                                    <a class="dropdown-item" href="/user/changepassword">Change
                                                        password</a>
                                                    {{#ifCond user.role 1}}
                                                    <a class="dropdown-item"
                                                        href="/vendor/manageproduct/{{user._id}}">Manage Product</a>
                                                    <a class="dropdown-item"
                                                        href="/vendor/manageorder/{{user._id}}">Manage Order</a>
                                                    {{/ifCond}}
                                                    {{#ifCond user.role 2}}
                                                    <a class="dropdown-item"
                                                        href="/admin/manageuser">Manage User</a>
                                                    <a class="dropdown-item"
                                                        href="/admin/managevoucher">Manage Voucher</a>
                                                    {{/ifCond}}
                                                    <a class="dropdown-item" href="/user/logout">Logout</a>
                                                </div>
                                                {{else}}
                                                <div class="dropdown-menu" aria-labelledby="dropdownMenu2">
                                                    <a class="dropdown-item" href="/login">Login</a>
                                                    <a class="dropdown-item" href="/login/signup">Signup</a>
                                                </div>
                                            </div>
                                            {{/if}}
                                        </li>
                                        <li>
                                            <a href="/user/cart"><img style="margin-right: 15px;" src="/icon/3.png"
                                                    alt="#" />
                                            </a>
                                        </li>

                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
        </div>
        <!--header end here-->
        {{{body}}}
        <!--footer strat here-->
        <footer>
            <div class="footer">
                <div class="container-fluid">
                    <div class="border1">
                        <div class="row">
                            <div class="col-xl-3 col-lg-3 col-md-3 col-sm-12">

                                <img class="logo1" src="/images/logo1.jpg" />
                            </div>

                            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12">

                                <ul class="lik">
                                    <li> <a href="/">Home</a></li>
                                    <li> <a href="/about">About</a></li>
                                    <li> <a href="/product">Product</a></li>
                                </ul>
                            </div>

                            <div class="col-xl-3 col-lg-3 col-md-3 col-sm-12">
                                <ul class="sociel">
                                    <li> <a href="#"><i class="fa fa-facebook-f"></i></a></li>
                                    <li> <a href="#"><i class="fa fa-twitter"></i></a></li>
                                    <li> <a href="#"><i class="fa fa-instagram"></i></a></li>
                                    <li> <a href="#"><i class="fa fa-linkedin"></i></a></li>
                                </ul>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </footer>
        <!-- end footer -->
    </div>
</body>

</html>

